<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/on_boarding_navigation"
    app:startDestination="@id/userSetupFragment">

    <fragment
        android:id="@+id/userSetupFragment"
        android:name="com.unlone.app.ui.access.UserSetupFragment"
        android:label="fragment_user_setup"
        tools:layout="@layout/fragment_user_setup" >
        <action
            android:id="@+id/toChooseCategoriesFragment"
            app:destination="@id/chooseCategoriesFragment" />
    </fragment>
    <fragment
        android:id="@+id/chooseCategoriesFragment"
        android:name="com.unlone.app.ui.access.ChooseCategoriesFragment"
        android:label="fragment_choose_categories"
        tools:layout="@layout/fragment_choose_categories" >
        <argument
            android:name="user"
            app:argType="com.unlone.app.instance.User" />
    </fragment>
</navigation>